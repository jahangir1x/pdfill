{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Qt Project",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Unnamed-Debug/pdfill",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Unnamed-Debug/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Project",
            "logging": {
                "engineLogging": true
            },
            "launchCompleteCommand": "exec-run",
            "miDebuggerServerAddress": "",
            "sourceFileMap": {
                "/build/qt-everywhere-src-5.15.2/qtbase/src": "${workspaceFolder}/qtbase/src"
            },
            "pipeTransport": {
                "pipeProgram": "bash",
                "pipeArgs": [
                    "-c"
                ],
                "debuggerPath": "/usr/bin/gdb"
            },
            "targetArchitecture": "x86_64"
        }
    ]
}